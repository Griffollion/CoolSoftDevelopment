html {
  box-sizing: border-box;
}
*,
*::before,
*::after {
  box-sizing: inherit;
}
body {
  font-family: Arial, Helvetica, sans-serif;
  font-weight: 400;
  font-size: 17px;
  white-space: nowrap;
  color: rgba(0, 0, 0, 1);
  background: url(sky-1.HASH_REF_f3b2275809fa887fb52c7398651ff62e.jpg) no-repeat;
  background-size: cover;
}
.wrapper {
  width: 1400px;
  margin: 0 auto;
  max-width: 100%;
  padding: 0 10px 50px 10px;
}
/* slyles header */
.header {
  padding: 20px;
  display: flex;
  justify-content:space-around;
  align-items: center;
}
.logo {
  display: inline-block;
}
.logo a {
  font-size: 30px;
  font-weight: 700;
  color: gray;
  text-decoration: none;
}
.logo:hover a {
  color: honeydew;
  transition: color 0.3s ease;
}
.icon-users{
  background: gray;
  font-weight: 600;
  color: antiquewhite;
  padding: 10px 30px;
  border-radius: 15px;
}
.icon-users:hover{
  cursor: pointer;
  background: antiquewhite;
  color: gray;
  transition: color 0.3s ease, background 0.3s ease;
}
body{
    color: #172b4d;
    font-size: 14px;
    line-height: 20px;
    font-weight: 400;
}

.button{
    display: inline-flex;
    align-items: center;
    justify-content: center;
    border-radius: 3px;
    cursor: pointer;
    padding: 6px 12px;
    background-color: rgba(9,30,66,.04);
}

.button:hover{
    background-color: rgba(9, 30, 66, 0.205);
}

.new__card{
    min-height: 600px;
    background: #f4f5f7;
    border-radius: 2px;
    margin: 80px auto;
    overflow: hidden;
    position: relative;
    width: 768px;
    padding: 20px;
}

.new__card-header{
    display: flex;
    justify-content: space-between;
    margin-bottom: 30px;
}

.new__card-header-name{
    font-size: 20px;
    font-weight: 600;
    margin-bottom: 10px;
}

.new__card-header-ststus-span{
    border-bottom: 1px solid;
    cursor: pointer; 
}

.new__card-header-ststus-span:hover{
    color: black;
}

.new__card-wrapper{
    display: flex;
    justify-content: space-between;
}

.card-info__container{
    display: flex;
    margin-bottom: 30px;
}

.new__card-info{
    min-width: 500px;
    margin-right: 30px;
}

.card-info__container-users{
    margin-right: 30px;
}

.card-info__container-users-title, .card-info__container-term-title{
    color: #5e6c84;
    font-size: 12px;
    font-weight: 500;
    margin-bottom: 5px;
}

.card-info__container-term-wrapper{
    display: flex;
    justify-content: center;
    align-items: center;
}

.card-info__container-term-input{
    margin-right: 5px;
}

.card-info__container-term-btn{
    display: inline-flex;
    justify-content: center;
    align-items: center;
    border-radius: 3px;
    cursor: pointer;
    padding: 6px 12px;
    background-color: rgba(9, 30, 66, 0.04);
}

.container-term-btn__data, .container-term-btn__status{
    margin-right: 7px;
}

.card-info__container-term-btn:hover{
    background-color: rgba(9, 30, 66, 0.205);
}

.card-info__description-wrapper, .card-info__actions-wrapper{
    display: flex;
    align-items: center;
    margin-bottom: 15px;
}

.card-info__description-title, .card-info__actions-title{
    font-size: 17px;
    font-weight: 600;
    margin-right: 10px;
}

.textarea-description{
    cursor: pointer;
    border: none;
    outline: none;
    background-color: #f4f5f7;
    width: 100%;
    margin: 0;
}

.textarea-actions{
    cursor: pointer;
    width: 100%;
    margin: 0;
}

.card-create {
    display: flex;
    flex-direction: column;
    padding: 0;
    margin: 0;
    margin-bottom: 20px
}

.card-create:last-child{
    margin-bottom: 0;
}

.card-create__title{
    color: #5e6c84;
    font-size: 12px;
    font-weight: 500;
    margin-bottom: 5px;
}

.card-create__button {
    margin-bottom: 7px;
    justify-content: left;
}

.button__white{
    background-color: #f4f5f7;
}
